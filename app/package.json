{
  "name": "kaja-twirp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean-modules": "clean-modules clean --yes",
    "start": "NODE_ENV=development nodemon src/server/main.ts -w src/server",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "protoc": "protoc --ts_opt=client_none --ts_opt=generate_dependencies --ts_out ./src/shared --twirp_ts_out ./src/shared --proto_path proto proto/api.proto"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.0",
    "@primer/octicons-react": "^19.8.0",
    "@primer/react": "^36.2.0",
    "@protobuf-ts/plugin": "2.9.1",
    "@protobuf-ts/twirp-transport": "^2.8.3",
    "@vitejs/plugin-react": "^3.0.1",
    "clean-modules": "^3.0.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.10",
    "ts-node": "^10.9.2",
    "twirp-ts": "^2.5.0",
    "typescript": "^5.3.2",
    "vite": "^4.0.4",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "vitest": "^0.34.6"
  }
